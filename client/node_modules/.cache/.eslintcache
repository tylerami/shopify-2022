console.log(err.toString());\n      });\n  };\n\n  return (\n    <div className=\"App\">\n      <div className=\"header\">\n        <h1>LogistX Inventory Tracker</h1>\n      </div>\n      <InventoryForm getItems={getItems} />\n      <h3 className=\"subheader\">Manage Inventory</h3>\n      <div className=\"divider\"></div>\n      <div className=\"inventory\">\n        <button className=\"large-button\" onClick={resetValues}>\n          Load Inventory\n        </button>\n        <button className=\"large-button\" onClick={getItems}>\n          Load Inventory\n        </button>\n        {itemsList.length === 0 ? (\n          <div></div>\n        ) : (\n          <div className=\"columns\">\n            <h4>Item ID</h4>\n            <h4>Name</h4>\n            <h4>Quantity</h4>\n            <h4>Unit Price</h4>\n            <h4>Location</h4>\n            <h4>Last Update</h4>\n\n            <h4></h4>\n          </div>\n        )}\n        {itemsList.map((val, key) => {\n          return (\n            <Item\n              key={val.id}\n              id={val.id}\n              name={val.name}\n              quantity={val.quantity}\n              unitValue={val.unitValue}\n              location={val.location}\n              lastUpdate={val.updatedAt.substring(0, 10)}\n              refresh={getItems}\n            />\n          );\n        })}\n      </div>\n      <div className=\"footer\"></div>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\Tyler\\Desktop\\Shopify Project\\client\\src\\components\\item.jsx",["24","25","26"],"C:\\Users\\Tyler\\Desktop\\Shopify Project\\client\\src\\components\\Item.jsx",[],"C:\\Users\\Tyler\\Desktop\\Shopify Project\\client\\src\\components\\InventoryForm.jsx",[],{"ruleId":"27","severity":1,"message":"28","line":193,"column":13,"nodeType":"29","endLine":193,"endColumn":17},{"ruleId":"30","severity":1,"message":"31","line":5,"column":20,"nodeType":"32","messageId":"33","endLine":5,"endColumn":28},{"ruleId":"30","severity":1,"mess